    {
      "name": "tp6_exo1.sh",
      "lines": [
        "#! /bin/bash\n",
        "if [ $1 -ge 5 -a $1 -le 10 ]\n",
        "then\n",
        "echo La valeur $1 est comprise entre 5 et 10\n",
        "else\n",
        "echo La valeur $1 nest pas comprise entre 5 et 10\n",
        "fi\n",
        "if test $1 -ge 5 -a $1 -le 10 \n",
        "then\n",
        "echo La valeur $1 est comprise entre 5 et 10\n",
        "else\n",
        "echo la valeur $1 nest pas comprise entre 5 et 10\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo2.sh",
      "lines": [
        "#! /bin/bash\n",
        "if test -r $1 -a -w $1\n",
        "then\n",
        "echo Le fichier $1 existe et il est accessible en mode mecture ecriture\n",
        "else\n",
        "echo Le fichier $1 nexiste pas ou nest pas en mode lecture ecriture\n",
        "fi\n",
        "\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#! /bin/bash\n",
        "if test -e $1 -a -e $2\n",
        "then\n",
        "echo Voulez vous concatener les fichier ?\n",
        "read test\n",
        "elif test $test = oui\n",
        "then\n",
        "cat $1 $2 >> F1\n",
        "cat F1\n",
        "else\n",
        "echo daccord\n",
        "fi\n"
      ]
    }
