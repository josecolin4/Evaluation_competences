  "scripts": [
    {
      "name": "tp6_exo1.sh",
      "lines": [
        "#!/bin/bash\n",
        "n1=5\n",
        "n2=10\n",
        "if [ $1 -gt $n1  -a  $1 -lt $n2 ]\n",
        "then\n",
        "\techo \"La valeur $1 est comprise entre 5 et 10\"\n",
        "else\n",
        "\techo \"La valeur $1 n'est pas comprise entre 5 et 10\"\n",
        "fi\n"
      ]
    },
    {
      "name": "tp5bis_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "set -- `ls -l $1`\n",
        "echo $5\n"
      ]
    },
    {
      "name": "tp6_exo2.sh",
      "lines": [
        "#!/bin/bash\n",
        "\n",
        "if test -r $1 && test -w $1 && test -e $1\n",
        "then\n",
        "\techo \"Le fichier $1 existe et est accessible en lecture et en ecriture\"\n",
        "else\n",
        "\techo \"Le fichier $1 n'existe pas ou n'est pas accessible en lecture ou ecriture\"\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "\n",
        "if test !-e $1 -o !-e $2\n",
        "then\n",
        "\techo \"Ce script ne fonctionnera pas\"\n",
        "else\n",
        "\techo \"Souhaitez-vous concatener les deux fichiers ?\"\n",
        "\tread reponse\n",
        "fi\n",
        "\n",
        "if response=\"Oui\"\n",
        "then\n",
        "\techo $1$2\n",
        "fi\n",
        "\n"
      ]
    }
  ],
