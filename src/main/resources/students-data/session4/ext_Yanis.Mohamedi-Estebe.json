    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "nbarguments=0\n",
        "for i in $*\n",
        "do\n",
        "\tnbarguments=$(( nbarguments + 1 ))\n",
        "\techo argument $nbarguments  \\= $i\n",
        "done \n",
        "echo Il y a $# arguments.\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "#/bin/bash\n",
        "factorielle=1\n",
        "for i in $(seq $1)\n",
        "do\n",
        "\tfactorielle=$(( factorielle * i ))\n",
        "done\n",
        "echo $factorielle\n"
      ]
    },
    {
      "name": "tp6_exo7.sh",
      "lines": [
        "#/bin/bash\n",
        "echo Liste des commandes disponibles :\n",
        "echo 1- Date du jour\n",
        "echo 2- Liste des personnes connect\u00c3\u00a9es sur la machine\n",
        "echo 3- Liste des fichiers du r\u00c3\u00a9pertoire courant\n",
        "echo 4- Nom du r\u00c3\u00a9pertoire courant\n",
        "echo\n",
        "echo Faites votre choix :\n",
        "read reponse\n",
        "if test $reponse = \"1\"\n",
        "\tthen\n",
        "\t\techo `date`\n",
        "\telif test $reponse = \"2\"\n",
        "\t\tthen\n",
        "\t\t\techo $(who)\n",
        "\telif test $reponse = \"3\"\n",
        "\t\tthen\n",
        "\t\t\techo $(ls)\n",
        "\telif test $reponse = \"4\"\n",
        "\t\tthen\n",
        "\t\t\techo $(pwd)\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "#/bin/bash\n",
        "chaine=\"o\"\n",
        "while test $chaine != \"sortir\"\n",
        "\tdo\n",
        "\t\tread chaine\n",
        "\t\techo $chaine\n",
        "done\n",
        "echo boucle finie\n",
        "\n"
      ]
    }
