    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "count=0\n",
        "for i in $*\n",
        "do\n",
        "\tcount=$(($count + 1))\n",
        "\techo \"argument \" $count \" = \" $i\n",
        "done\n",
        "echo \"Il y a \" $count \" arguments\"\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "#!/bin/bash\n",
        "factorielle=1\n",
        "for i in $(seq $1)\n",
        "do\n",
        "\tfactorielle=$((($factorielle)*($i)))\n",
        "done\n",
        "echo $factorielle\n"
      ]
    },
    {
      "name": "tp6_exo7.sh",
      "lines": [
        "#!/bin/bash\n",
        "echo \"Liste des commandes disponibles :\"\n",
        "echo \"1 - Date du jour\"\n",
        "echo \"2 - Liste des personnes connect\u00c3\u00a9es sur la machine\"\n",
        "echo \"3 - Liste des fichiers du r\u00c3\u00a9pertoire courant\"\n",
        "echo \"4 - Nom du r\u00c3\u00a9pertoire courant\"\n",
        "echo \"\"\n",
        "echo \"Faites votre choix\"\n",
        "read choix\n",
        "echo \"\"\n",
        "case $choix in\n",
        "\t1 ) echo `date` ;;\n",
        "\t2 ) echo `who` ;;\n",
        "\t3 ) echo `ls -l` ;;\n",
        "\t4 ) echo `pwd` ;;\n",
        "esac\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "#!/bin/bash\n",
        "read mot\n",
        "while ((mot!=\"sortir\"))\n",
        "do\n",
        "\tread mot\n",
        "done\n"
      ]
    }
