    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "if test -e $1 -a -e $2\n",
        "   then\n",
        "echo Souhaitez vous concat\u00c3\u00a9ner les deux fichiers \\?\n",
        "read rep\n",
        "  if test $rep = \"OUI\" \n",
        "    then\n",
        "      cat $1 $2\n",
        "  fi\n",
        "\n",
        " else\n",
        "echo Un des fichiers ou les deux n\\'existe\\(nt\\) pas\n",
        "fi\n",
        "\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "#!/bin/bash\n",
        "set $(cat $1)\n",
        "for i in $*\n",
        "do\n",
        "$i\n",
        "done\n"
      ]
    },
