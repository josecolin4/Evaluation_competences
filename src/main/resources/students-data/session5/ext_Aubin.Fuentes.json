    {
      "name": "tp6_exo1.sh",
      "lines": [
        "#!/bin/bash\n",
        "if [ $1 -gt 5 -a $1 -lt 10 ]\n",
        "\tthen\n",
        "\t\techo La valeur $1 est comprise entre 5 et 10\n",
        "else\n",
        "\t\techo La valeur $1 n\\'est pas comprise entre 5 et 10\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo2.sh",
      "lines": [
        "#!/bin/bash\n",
        "if [ -r $1 -a -w $1 ]\n",
        "\tthen\n",
        "\t\techo Le fichier $1 existe et est accessible en lecture et en \u00c3\u00a9criture\n",
        "\telse\n",
        "\t\techo Le fichier $1 n\\'existe pas ou n\\'est pas accessible, ou n\\'est pas accessible en \u00c3\u00a9criture \n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "if [ -e $1 -a -e $2 ]\n",
        "\tthen\n",
        "\t\techo Souhaitez-vous concat\u00c3\u00a9ner les fichiers ?\n",
        "\t\tread reponse\n",
        "\t\tif [ $reponse = \"OUI\" -o $reponse = \"oui\" -o $reponse = \"Oui\" ]\n",
        "\t\t\tthen\n",
        "\t\t\t\techo \"${1}${2}\"\n",
        "\t\t\telse\n",
        "\t\t\t\techo erreur\n",
        "\t\tfi\n",
        "\telse\n",
        "\t\techo Erreur : Un des fichiers n\\'existe pas\n",
        "fi\n"
      ]
    }
  ],
