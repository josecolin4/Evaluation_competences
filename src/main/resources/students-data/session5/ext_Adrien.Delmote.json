      "name": "tp6_exo8.sh",
      "lines": [
        "echo quelle est votre character \n",
        "read chaine\n",
        "phrase=\"\"\n",
        "\n",
        "while [ $chaine != \"sortir\" ] \n",
        "do\n",
        "\tphrase+=\"$chaine \"\n",
        "\techo quelle est votre character \n",
        "\tread chaine\n",
        "done\n",
        "echo $phrase\n",
        "\n"
      ]
    },
    {
      "name": "tp6_exo9.sh",
      "lines": [
        "echo quelle numero ?\n",
        "read rep \n",
        "while test $rep -le 4 -a $rep -ge 1\n",
        "do\n",
        "case $rep in\n",
        "\t1 ) date;;\n",
        "\t2 ) who;;\n",
        "\t3 ) ls;;\n",
        "\t4 ) pwd;;\n",
        "esac\n",
        "echo nouveau numero\n",
        "read rep\n",
        "done\n",
        "\n"
      ]
    },
