    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#! /bin/bash\n",
        "if test -e $1 -a -e $2\n",
        "then\n",
        "echo Voulez vous concatener les fichier ?\n",
        "read reponse\n",
        "\n",
        "\tif test $reponse = OUI\n",
        "\tthen\n",
        "\t\tcat $1 $2 >> F1\n",
        "\t\tcat F1\n",
        "\tfi\n",
        "else\n",
        "echo Le fichier existe pas\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "#! /bin/bash\n",
        "for liste_commande in date who pwd ls\n",
        "do\n",
        "\t$liste_commande\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#! /bin/bash\n",
        "i=1\n",
        "for x in toto tata tutu \n",
        "do \n",
        "\techo $i = $x\n",
        "\ti=$(( i+1 ))\n",
        "done\n",
        "echo Il y a $# arguments\n"
      ]
    },
