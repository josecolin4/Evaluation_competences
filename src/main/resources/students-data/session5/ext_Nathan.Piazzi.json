    {
      "name": "tp6_exo8.sh",
      "lines": [
        "phrase=\"\"\n",
        "echo Quel est la chaine a ecrire :\n",
        "read n\n",
        "while [ $n != \"sortir\" ]\n",
        "do\n",
        "phrase+=\"$n \"\n",
        "echo Quel est le nouveau mot a ecrire :\n",
        "read n\n",
        "done\n",
        "echo $phrase\n"
      ]
    },
    {
      "name": "tp6_exo9.sh",
      "lines": [
        "echo Que voulez vous voir \n",
        "read n\n",
        "while test $n -le 4 -a $n -ge 1 \n",
        "do\n",
        "case $n in\n",
        "\t\"1\") date\n",
        "\t;;\n",
        "\t\"2\") who\n",
        "\t;;\n",
        "\t\"3\") ls\n",
        "\t;;\n",
        "\t\"4\") pwd\n",
        "\t;;\n",
        "esac\n",
        "echo Que voulez voir ?\n",
        "read n\n",
        "done\n"
      ]
    },
