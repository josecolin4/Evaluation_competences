      "name": "tp6exo4.sh",
      "lines": [
        "echo x\n",
        "echo $listec date who pwd ls\n",
        "for listec in date who pwd ls\n",
        "\tdo $listec\n",
        "done\n"
      ]
    },
    {
      "name": "tp6exo5.sh",
      "lines": [
        "a=\"cc\"\n",
        "b=\"o\"\n",
        "c=\"p\"\n",
        "d=1\n",
        "e=2\n",
        "f=3\n",
        "for i in $a $b $c $d $e $f\n",
        "do\n",
        "  echo argument $i+3  =$i\n",
        "done\n",
        "echo il y a trois arguments\n"
      ]
    },
    {
