    {
      "name": "tp5bis_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "echo Nom du fichier :\n",
        "read file\n",
        "set -- $(ls -l $file) \n",
        "echo $1 $file $6\n",
        "echo $2\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "echo Rentrer 1 fichier\n",
        "read file1 \n",
        "echo Rentrer 1 autre fichier\n",
        "read file2\n",
        "if test -e $file1 -a -e $file2\n",
        "\tthen \n",
        "\t\techo Souhaitez-vous  concatener les fichiers ?\n",
        "\t\tread Rep\n",
        "\t\tcase $Rep in\n",
        "\t\t\toui) echo $file1 $file2 ;;\n",
        "\t\t\tnon) echo Erreur ;;\n",
        "\t\tesac\n",
        "else\n",
        "\techo Au moins 1 des fichiers n\\'existe pas. R\u00c3\u00a9essayer \n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "liste_commandes=\"date who pwd ls\"\n",
        "for i in $liste_commandes\n",
        "do\n",
        "\t$i\n",
        "done\n"
      ]
    },
