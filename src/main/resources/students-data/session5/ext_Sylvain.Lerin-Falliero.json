      "name": "tp6_exo5.sh",
      "lines": [
        "compteur=0\n",
        "for i in $*\n",
        "do\n",
        "compteur=`expr $compteur + 1`\n",
        "echo Argument $compteur = $i\n",
        "done\n",
        "echo Il y a $# arguments.\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "factorielle=1\n",
        "for i in `seq $1`\n",
        "do\n",
        "factorielle=`expr $factorielle \\* $i`\n",
        "done\n",
        "echo $factorielle\n"
      ]
    },
