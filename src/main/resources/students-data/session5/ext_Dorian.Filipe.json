    {
      "name": "tp6_exo7.sh",
      "lines": [
        "case $1 in\n",
        "\t\"1\") date;;\n",
        "\t\"2\") who;;\n",
        "\t\"3\") ls;;\n",
        "\t\"4\") pwd;;\n",
        "esac\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "phrase=\"\"\n",
        "string=string\n",
        "while [ $string != \"sortir\" ]\n",
        "do\n",
        "\techo Tapez une chaine de caractere\n",
        "\tread string\n",
        "\tphrase+=\"$string \"\n",
        "done\n",
        "echo $phrase\n"
      ]
    },
    {
      "name": "tp6_exo9.sh",
      "lines": [
        "rep=$1\n",
        "while test $rep -ge 1 -a $rep -le 4\n",
        "do\n",
        "case $rep in\n",
        "\t\"1\") date;;\n",
        "\t\"2\") who;;\n",
        "\t\"3\") ls;;\n",
        "\t\"4\") pwd;;\n",
        "esac\n",
        "\techo \"Saisissez une commande (1,2,3, ou 4) :\"\n",
        "\tread rep\n",
        "done\n"
      ]
    },
    {
