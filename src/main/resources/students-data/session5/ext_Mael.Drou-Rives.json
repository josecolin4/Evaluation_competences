    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "x=1\n",
        "for i in $*\n",
        "do\n",
        "  echo \"argument\" $i \"=\" $x\n",
        "  let x++\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "#!/bin/bash\n",
        "p=1\n",
        "for i in $(seq $1)\n",
        "do\n",
        "  p=$((($p)*($i)))\n",
        "done \n",
        "echo $p\n",
        "\n"
      ]
    },
    {
      "name": "tp6_exo7.sh",
      "lines": [
        "#!/bin/bash\n",
        "echo \" 1 - la date \"\n",
        "echo \" 2 - le nom de l'utilisateur \"\n",
        "echo \" 3 - la liste du repertoire courant \"\n",
        "echo \" 4 - le nom du repertoire courant \"\n",
        "read choix\n",
        "case $choix in\n",
        "1 ) echo `date`;;\n",
        "2 ) echo `who`;;\n",
        "3 ) echo `ls -l`;;\n",
        "4 ) echo `pwd`;;\n",
        "esac\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "#!/bin/bash\n",
        "read chaine\n",
        "while $(($chaine != sortir))\n",
        "do\n",
        "\techo $chaine\n",
        "\tread chaine\n",
        "done\n"
      ]
    }
  ],
