    {
      "name": "tp6_exo2.sh",
      "lines": [
        "#!/bin/bash\n",
        "if [ -e $1 -a -r $1 -a -w $1 ]\n",
        "then\n",
        "echo Le fichier $1 existe et est accessible en lecture et \u00c3\u00a9criture\n",
        "else\n",
        "echo Le fichier $1 n existe pas, ou n est pas accessible en lecture,\n",
        "echo ou n est pas accessible en \u00c3\u00a9criture\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "if [ -e $1 -a -e $2 ]\n",
        "then\n",
        "echo souhaitez-vous concat\u00c3\u00a9ner les fichiers \\?\n",
        "read reponse\n",
        "if test $reponse = \"oui\"\n",
        "then\n",
        "echo $(cat $1 $2)\n",
        "else\n",
        "echo erreur, r\u00c3\u00a9ponse inattendue\\!\n",
        "fi\n",
        "else\n",
        "echo erreur, un des fichiers n\\'existe pas\\!\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "#!/bin/bash\n",
        "liste_commandes=\"date who pwd ls\"\n",
        "for i in $liste_commandes\n",
        "do\n",
        "  $i\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "compteur=1\n",
        "for i in $*\n",
        "do\n",
        "  echo argument$compteur = $i\n",
        "  compteur=$(($compteur + 1))\n",
        "done\n",
        "echo Il y a $# arguments\\.\n",
        "\n"
      ]
    }
