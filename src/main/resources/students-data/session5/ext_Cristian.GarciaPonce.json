        "#!/bin/bash\n",
        "\n",
        "liste_commandes=\"date who pwd ls\"\n",
        "set $(echo $liste_commandes)\n",
        "$1\n",
        "$2\n",
        "$3\n",
        "$4\n",
        "for i in 9 8 7 6 5 4 3 2 1\n",
        "do\n",
        "\techo $i\n",
        "done\n"
      ]
    },
