      "name": "tp6_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "if [ -e $1 -a -e $2 ]\n",
        "\tthen\n",
        "\t\techo \"Souhaitez-vous concat\u00c3\u00a9ner les fichiers ?\"\n",
        "\t\tread reponse\n",
        "\tfi\n",
        "\n",
        "if [ $reponse = \"oui\" ]\n",
        "\tthen\n",
        "\t\tcat $1 $2\n",
        "\tfi\n",
        "\n"
      ]
    },
    {
