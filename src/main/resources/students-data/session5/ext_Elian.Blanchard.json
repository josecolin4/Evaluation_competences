        "#!/bin/sh\n",
        "fct(){   \n",
        "\tvar2=\"variable 2\"\n",
        "\techo \"\\nDans fonction\"\n",
        "\techo \"\\$1 = $1\"\n",
        "\techo \"var1 = $var1\"\n",
        "\techo \"var2 = $var2\"\n",
        "\techo \"fin fonction\\n\"}\n",
        "\tvar1=\"variable 1\"\n",
        "\techo \"dans script principal\"\n",
        "\techo \"\\$1 = $1\"\n",
        "\techo \"var1 = $var1\"\n",
        "\tfct $var1\n",
        "\techo \"var2 = $var2\"\n",
        "\techo \"fin script principal\"\n"
      ]
    }
