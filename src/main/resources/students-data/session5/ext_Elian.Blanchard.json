      "lines": [
        "\n",
        "case $1 in\n",
        "\t\"1\") date\n",
        "\t;;\n",
        "\t\"2\") who\n",
        "\t;;\n",
        "\t\"3\") ls\n",
        "\t;;\n",
        "\t\"4\") pwd\n",
        "\t;;\n",
        "esac\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "echo saisissez une chaine de character\n",
        "read rep\n",
        "\n",
        "while [ $rep != \"sortir\" ]\n",
        "do\n",
        "\tphrase+=\"$rep \"\n",
        "\techo saisissez une chaine de character\n",
        "\tread rep\n",
        "done\n",
        "echo \"$phrase\"\n"
      ]
    },
    {
      "name": "tp6_exo9.sh",
      "lines": [
        "echo quelle numero ?\n",
        "read rep\n",
        "while test $rep -le 4 -a $rep -ge 1\n",
        "do\n",
        "case \"$rep\" in\n",
        "\t\"1\") date\n",
        "\t;;\n",
        "\t\"2\") who\n",
        "\t;;\n",
        "\t\"3\") ls\n",
        "\t;; \n",
        "\t\"4\") pwd\n",
        "\t;;\n",
        "esac\n",
        "echo saisissez la valeur\n",
        "read rep\n",
        "done\n"
      ]
    },
