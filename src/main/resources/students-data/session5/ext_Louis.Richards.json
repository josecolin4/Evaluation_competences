    [
      "#!/bin/bash\n",
      "calcul(){\n",
      "operateur=$3\n",
      "\n",
      "case operateur in\n",
      "\t+ ) result=`expr($1 + $2)` ;;\n",
      "\t- ) result=`expr($1 - $2)` ;;\n",
      "\t/ ) result=`expr($1 / $2)` ;;\n",
      "\t\\* ) result=`expr($1 \\*$2)` ;;\n",
      "esac\n",
      "\n",
      "echo $result\n",
      "}\n",
      "echo Le resultat de l\\'op\\\u00c3\u00a9ration est :\n",
      "calcul $1 $2 $3\n"
