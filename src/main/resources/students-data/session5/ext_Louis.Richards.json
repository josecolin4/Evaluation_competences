      "\n",
      "calcul(){\n",
      "\tcase \"$2\" in\n",
      "\t\t\"+\") echo (($1 + $3));;\n",
      "\t\t\"-\") echo (($1 - $3));;\n",
      "\t\t\"*\") echo (($1 * $3));;\n",
      "\t\t\"/\") echo (($1 / $3));;\n",
      "\tesac\n",
      "}\n",
      "\n",
      "calcul\n"
