    {
      "name": "tp6_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "if test -e $1 -a -e $2\n",
        "then\n",
        "\techo Souhaitez vous concat\u00c3\u00a9ner les fichiers ?\n",
        "read rep\n",
        "\tif test $rep = oui\n",
        "\tthen\n",
        "\t\tcat $1 $2 > fic\n",
        "\t\tcat fic\n",
        "\telse\n",
        "\t\techo erreur\n",
        "\tfi\n",
        "else\n",
        "\techo au moins 1 des deux fichiers n\\'existe pas\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "#!/bin/bash\n",
        "for liste_commande in date who pwd ls\n",
        "do\n",
        "\t$liste_commande\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo5.sh",
      "lines": [
        "#!/bin/bash\n",
        "for arg in $1\n",
        "do\n",
        "for ar in $2\n",
        "do\n",
        "for a in $3\n",
        "do\n",
        "\techo argument 1 = $arg, argument 2 = $ar, argument 3 = $a. il y a : $# arguments\n",
        "done\n",
        "done\n",
        "done\n",
        "\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "#!/bin/bash\n",
        "val=0\n",
        "for i in $(seq $1)\n",
        "\tval=$((val*$i))\n",
        "done\n",
        "echo $val\n"
      ]
    }
