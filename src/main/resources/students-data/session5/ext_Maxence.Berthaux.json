      "lines": [
        "#!/bin/bash\n",
        "echo Merci de rentr\u00c3\u00a9 le nom du 1er fichier\n",
        "read fi1\n",
        "echo Merci de rentr\u00c3\u00a9 le nom du 2eme fichier\n",
        "read fi2\n",
        "\n",
        "if test -e $fi1\n",
        "\tthen \n",
        "\techo Le fichier 1 existe\n",
        "else \n",
        "\techo Le fichier 1 n\\'esxiste pas\n",
        "fi\t\n",
        "if test -e $fi2\n",
        "\tthen\n",
        "\techo Le fichier 2 existe\n",
        "else \n",
        "\techo Le fichier 2 n\\'existe pas\n",
        "fi\n",
        "if test -e $fi1 -a -e $fi2\n",
        "\tthen \n",
        "\techo Souhaitez\\-vous concat\\\u00c3\u00a9ner les fichier \\?\n",
        "\tread rep\n",
        "\tcase $rep in \n",
        "\t\toui ) cat $fi1 $fi2 ;;\n",
        "\t\tnon ) echo \"Pas OK\" ;; \n",
        "\tesac\n",
        "else \n",
        "\techo Erreur\n",
        "fi\n",
        "\n"
      ]
    },
