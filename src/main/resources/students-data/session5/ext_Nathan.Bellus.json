  "scripts": [
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "if test -e $1 && test -e $2\n",
        "\n",
        "then echo \"Les 2 fichiers existent\"\n",
        "\n",
        "echo \"Voulez vous concat\u00c3\u00a9nez les fichiers ?\"\n",
        "read a\n",
        "\n",
        "if a=\"oui\"\n",
        " \n",
        "then  cat $1 $2 > fic0\n",
        "\n",
        "fi \n",
        "\n",
        "else echo \"ERROR 404\"\n",
        "\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "for liste_commande in date who pwd ls\n",
        "do\n",
        "$liste_commande\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo5.sh",
      "lines": [
        "for i in 3 2 1 \n",
        "\n",
        "do \n",
        "\n",
        "echo \"argument\" $i \n",
        "\n",
        "\n",
        "if (( $i == 1 ))\n",
        "\n",
        "then \n",
        "\n",
        "echo \"toto\" \n",
        "\n",
        "\n",
        "elif (( $i == 2 )) \n",
        "\n",
        "then\n",
        "\n",
        "echo \"titi\" \n",
        "\n",
        "\n",
        "else \n",
        "\n",
        "echo \"tutu\"\n",
        "\n",
        "\n",
        "fi\n",
        "\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo6.sh",
      "lines": [
        "$b=1\n",
        "for i in $(seq $1)\n",
        "do\n",
        "$b = expr (( $i * $b ))\n",
        "done\n",
        "\n"
      ]
    }
