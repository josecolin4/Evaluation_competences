      "lines": [
        "#!/bin/bash\n",
        "if test $1 -ge 5 -a $1 -le 10\n",
        "   then\n",
        "       echo La valeur $1 est comprise entre 5 et 10.\n",
        "   else\n",
        "       echo La valeur $1 n\u00e2\u20ac\u2122est pas comprise entre 5 et 10.\n",
        "fi\n"
      ]
    },
