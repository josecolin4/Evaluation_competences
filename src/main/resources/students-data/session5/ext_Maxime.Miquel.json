        "       echo Souhaitez vous concat\u00c3\u00a9ner ces fichiers ?\n",
        "       read reponse\n",
        "       case $reponse in\n",
        "            OUI ) cat $1 $2;;\n",
        "            NON ) echo ERREUR2.\n",
        "       esac\n",
        "   else\n",
        "       echo ERREUR1.\n",
        "fi\n",
        "\n"
      ]
    },
