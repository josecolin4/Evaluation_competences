      "lines": [
        "#!/bin/sh\n",
        "fct(){\n",
        "  var2=\"variable 2\"\n",
        "  echo \"\\nDans fonction\"\n",
        "  echo \"\\$1 = $1\"\n",
        "  echo \"var1 = $var1\"\n",
        "  echo \"var2 = $var2\"\n",
        "  echo \"fin fonction\\n\"\n",
        "}\n",
        "var1=\"variable 1\"\n",
        "echo \"dans script principal\"\n",
        "echo \"\\$1 = $1\"\n",
        "echo \"var1 = $var1\"\n",
        "fct $var1\n",
        "echo \"var2 = $var2\"\n",
        "echo \"fin script principal\"\n"
      ]
    },
    {
      "name": "tp7_exo2.sh",
      "lines": [
        "#!/bin/bash\n",
        "calcul(){\n",
        "  operateur=$3\n",
        "  case $operateur in\n",
        "\t  + ) result=`expr $1 + $2` ;;\n",
        "  \t  - ) result=`expr $1 - $2` ;;\n",
        "\t  / ) result=`expr $1 / $2` ;;\n",
        "\t  * ) result=`expr $1 \\* $2` ;;\n",
        "  esac\n",
        "  echo $result\n",
        "}\n",
        "\n",
        "echo Le resultat de l\\'op\\\u00c3\u00a9ration est :\n",
        "calcul $1 $2 $3\n"
      ]
    },
