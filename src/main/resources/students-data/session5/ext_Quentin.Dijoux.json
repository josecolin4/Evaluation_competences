    {
      "name": "tp6_exo6.sh",
      "lines": [
        "#!/bin/bash\n",
        "\n",
        "fact() {\n",
        "\tn=$1\n",
        "\tif [ $n -eq 0 ]\n",
        "\tthen\n",
        "\t\techo 1\n",
        "\telse\n",
        "\t\techo $(( n * `fact $(( n - 1 ))` ))\n",
        "\tfi\n",
        "}\n",
        "echo `fact $1`\n"
      ]
    },
    {
      "name": "tp6_exo7.sh",
      "lines": [
        "echo \"Veuillez choisir une option \"\n",
        "read choix\n",
        "case $choix in\n",
        "\t1) date;;\n",
        "\t2) who | wc -l;;\n",
        "\t3) ls ;;\n",
        "\t4) pwd;;\n",
        "esac\n"
      ]
    },
    {
      "name": "tp6_exo8.sh",
      "lines": [
        "echo \"entrer une chaine:\"\n",
        "read chaine1\n",
        "while [ $chaine1 != Sortir ] \n",
        "do\n",
        "\techo \"encore:\"\n",
        "\tread chaine1\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo9.sh",
      "lines": [
        "echo \"Veuillez choisir une option:\"\n",
        "read choix\n",
        "while [ $choix -ge 1 ] && [ $choix -le 4 ] \n",
        "do\n",
        "\tcase $choix in\n",
        "\t\t1) date;;\n",
        "\t\t2) who | wc -l;;\n",
        "\t\t3) ls;;\n",
        "\t\t4) pwd;;\n",
        "\tesac\n",
        "\techo \"Veillez choisir une autre option:\"\n",
        "\tread choix\n",
        "done\n"
      ]
    }
