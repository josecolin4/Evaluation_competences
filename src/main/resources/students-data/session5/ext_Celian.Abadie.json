    {
      "name": "tp7_exo1.sh",
      "lines": [
        "#!/bin/sh\n",
        "fct(){\n",
        " var2=\"variable 2\"\n",
        " echo \"\\nDans fonction\"\n",
        " echo \"\\$1 = $1\"\n",
        " echo \"var1 = $var1\"\n",
        " echo \"var2 = $var2\"\n",
        " echo \"fin fonction\\n\"\n",
        "}\n",
        "\n",
        "var1=\"variable 1\"\n",
        "echo \"dans script principal\"\n",
        "echo \"\\$1 = $1\"\n",
        "echo \"var1 = $var1\"\n",
        "fct $var1\n",
        "echo \"var2 = $var2\"\n",
        "echo \"fin script principal\"\n"
      ]
    },
    {
      "name": "tp7_exo2.sh",
      "lines": [
        "#!/bin/bash\n",
        "calcul() {\n",
        "nb=0\n",
        "\tif [ $2 = \"*\" ]\n",
        "\t\tthen\n",
        "\t\t\tnb=`expr $1 \\* $3`\n",
        "\t\telif [ $2 = \"/\" ]\n",
        "\t\t\tthen\n",
        "\t\t\t\tnb=`expr $1 \\/ $3`\n",
        "\t\telif [ $2 = \"+\" ]\n",
        "\t\t\tthen\n",
        "\t\t\t\tnb=`expr $1 \\+ $3`\n",
        "\t\telif [ $2 = \"-\" ]\n",
        "\t\t\tthen\n",
        "\t\t\t\tnb=`expr $1 \\- $3`\n",
        "\tfi\n",
        "echo Resultat \\: $nb\n",
        "}\n",
        "calcul $1 $2 $3\n"
      ]
    },
    {
      "name": "tp7_exo3.sh",
      "lines": [
        "#!/bin/bash\n",
        "factorielle() {\n",
        "\tif [ $1 -eq 0 -o $1 -eq 1 ]\n",
        "\t\tthen\n",
        "\t\t\tnbr=1\n",
        "\t\telse\n",
        "\t\t\tlast=$(factorielle $(( $1 - 1)))\n",
        "\t\t\tnbr=$(( $1 * last))\n",
        "\tfi\n",
        "echo $nbr\n",
        "}\n",
        "factorielle $1\n"
      ]
    }
  ],
