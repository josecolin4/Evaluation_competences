    {
      "name": "tp6_exo9.sh",
      "lines": [
        "#!/bin/bash\n",
        "random=1\n",
        "while [ $random -eq 1 -o $random -eq 2 -o $random -eq 3 -o $random -eq 4 ]\n",
        "do\n",
        "\techo Liste des commandes disponibles :\n",
        "\techo 1- Date du jour\n",
        "\techo 2- Liste des personnes connect\u00c3\u00a9es sur la machine\n",
        "\techo 3- Liste des fichiers du r\u00c3\u00a9pertoires courant\n",
        "\techo 4- Nom du r\u00c3\u00a9pertoire courant\n",
        "\n",
        "\techo Faites votre choix: 1, 2, 3, 4, ou autre pour sortir\n",
        "\tread random\n",
        "\n",
        "\tcase $random in\n",
        "\t\t1 ) echo `date` ;;\n",
        "\t\t2 ) echo `who` ;;\n",
        "\t\t3 ) echo `ls -l` ;;\n",
        "\t\t4 ) echo `pwd` ;;\n",
        "\tesac\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo7.sh",
      "lines": [
        "#!/bin/bash\n",
        "echo Liste des commandes disponibles :\n",
        "echo 1- Date du jour\n",
        "echo 2- Liste des personnes connect\u00c3\u00a9es sur la machine\n",
        "echo 3- Liste des fichiers du r\u00c3\u00a9pertoire courant\n",
        "echo 4- Nom du r\u00c3\u00a9pertoire courant\n",
        "\n",
        "echo Faites votre choix : 1, 2, 3, ou 4\n",
        "read random\n",
        "\n",
        "case $random in\n",
        "\t1 ) echo `date` ;;\n",
        "\t2 ) echo `who` ;;\n",
        "\t3 ) echo `ls -l` ;;\n",
        "\t4 ) echo `pwd` ;;\n",
        "esac\n"
      ]
    },
