        "#!/bin/bash\n",
        "fct () {\n",
        "\tvar2=\"variable 2\"\n",
        "\techo \"\\nDans fonction\"\n",
        "\techo \"\\$1 = $1\"\n",
        "\techo \"var1 = $var1\"\n",
        "\techo \"var2 = $var2\"\n",
        "\techo \"fin fonction\\n\"\n",
        "}\n",
        "\n",
        "var1=\"variable1\"\n",
        "echo \"dans script principal\"\n",
        "echo \"\\$1 = $1\"\n",
        "echo \"var1 = $var1\"\n",
        "fct $var1\n",
        "echo \"var2 = $var2\"\n",
        "echo \"fin script principal\"\n"
      ]
    },
