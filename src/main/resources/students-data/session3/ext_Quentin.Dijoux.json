  "scripts": [
    {
      "name": "tp6_exo1.sh",
      "lines": [
        "if [ $1 -gt 5 ] && [ $1 -lt 10 ]\n",
        " then\n",
        "echo \"La valeur $1 est comprise entre 5 et 10\";\n",
        " else\n",
        "echo \"La valeur $1 n'est pas comprise\";\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo2.sh",
      "lines": [
        "if [ -e $1 ] && [ -r $1 ] && [ -w $1 ] \n",
        "then\n",
        "echo \"$1 existe et est accessible en lecture et en ecriture\";\n",
        "else\n",
        "echo \"$1 n'existe pas ou n'est pas accessible en lecture et en ecriture\";\n",
        "fi\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "if [ -e $1 ] && [ -e $2 ]\n",
        "then\n",
        " echo \"Voulez-vous voir le contenu des fichiers?\";\n",
        " read reponse;\n",
        "if [ $reponse = oui ] \n",
        "then\n",
        "  cat $1;\n",
        "  cat $2;\n",
        "fi\n",
        "else\n",
        " echo \"Erreur systeme\";\n",
        "fi\n",
        "\n"
      ]
    },
    {
      "name": "tp4_exo3.sh",
      "lines": [
        "while \n"
      ]
    },
    {
      "name": "tp6_exo4.sh",
      "lines": [
        "liste_commandes=\"date who pwd ls\"\n",
        "for cmd in $liste_commandes\n",
        " do\n",
        "\techo `$cmd`\n",
        "\n",
        "done\n"
      ]
    },
    {
      "name": "tp6_exo5.sh",
      "lines": [
        "\n",
        "for i in $*\n",
        "do\n",
        " echo \"Argument $i =\" $i\n",
        "done\n",
        "echo \"Il y a $# arguments\"\n"
      ]
    }
