  "scripts": [
    {
      "name": "tp6_exo1.sh",
      "lines": [
        "[ $1 -ge 5 -a $1 -le 10 ] && echo \"La valeur $1 est comprise entre 5 et 10\" || echo \"La valeur $1 n'est pas comprise entre 5 et 10\"\n"
      ]
    },
    {
      "name": "tp6_exo2.sh",
      "lines": [
        "[ -r $1 -a -w $1 ] && echo \"Le fichier $1 existe et est accessible en lecture et ecriture\" || echo \"$1 n'existe pas ou n'est pas accessible en lecture ou en ecriture\"\n"
      ]
    },
    {
      "name": "tp6_exo3.sh",
      "lines": [
        "[ -e $1 -a -e $2 ] && echo \"Les fichiers $1 et $2 existent\" || echo \"Au moins un des fichiers n'existent pas. D\u00c3\u00a9so pas d\u00c3\u00a9so\"\n",
        "echo Voulez-vous concat\\\u00c3\u00a9ner les fichiers \\?\n",
        "read O\n",
        "if [ $O=\"OUI\" ]\n",
        "then\n",
        "cat fichier1 fichier2 > fichier3\n",
        "cat fichier3\n",
        "if [ $O != \"OUI\" ]\n",
        "echo ERREUR\n",
        "fi\n"
      ]
    }
  ],
